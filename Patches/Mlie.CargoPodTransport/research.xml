<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Cargo Pod Transport (Continued)</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationRemove">
                    <xpath>Defs/ResearchTabDef[defName="CargoPod"]</xpath>
                </li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/ResearchProjectDef[defName="CargoPodBuild"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="Building_Helicopter"]/researchPrerequisites</xpath>
				</li>
            </operations>
        </match>
    </Operation>
</Patch>