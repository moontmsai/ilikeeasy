<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Furnace</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationRemove">
                    <xpath>Defs/ResearchProjectDef[defName="Furnace"]</xpath>
                </li>
                <li Class="PatchOperationRemove">
                    <xpath>Defs/ThingDef[defName="Furnace"]/researchPrerequisites</xpath>
                </li>
                <li Class="PatchOperationRemove">
                    <xpath>Defs/ThingDef[defName="ThermalVent"]/researchPrerequisites</xpath>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>