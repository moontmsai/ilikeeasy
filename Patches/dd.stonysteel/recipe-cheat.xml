<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Stonysteel</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs</xpath>
                    <value>
                        <RecipeDef>
                            <defName>MakeStonysteelCheats</defName>
                            <label>석철 x20 제작</label>
                            <description>석철을 제작한다. (x20)</description>
                            <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
                            <effectWorking>CutStone</effectWorking>
                            <soundWorking>Recipe_MakeStoneBlocks</soundWorking>
                            <targetCountAdjustment>20</targetCountAdjustment>
                            <workSkill>Crafting</workSkill>
                            <recipeUsers>
                                <li>MTCraftingSpot</li>
                            </recipeUsers>
                            <workAmount>11</workAmount>
                            <products>
                                <Stonysteel>20</Stonysteel>
                            </products>
                        </RecipeDef>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>

</Patch>