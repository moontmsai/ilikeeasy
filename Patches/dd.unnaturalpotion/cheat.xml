<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Unnatural potion</li>
        </mods>
        <match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<RecipeDef>
							<defName>Make_Unnaturalpotion_Cheat</defName>
							<label>부자연적인 물약 제작 x5</label>
