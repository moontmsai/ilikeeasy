<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Duct Tape! (Continued)</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>Defs</xpath>
					<value>
						<RecipeDef>
							<defName>MakeDT_DuctTape_cheat</defName>
							<label>덕트 테이프 x40 제작</label>
